
<%- include('partial_header.ejs'); %>
<main>
    <table class="table table-striped table-bordered table-hover">
        <thead>
            <tr>
                <td>Name Event</td>
                <td>Place</td>
                <td>Start time</td>
                <td>More</td>
            </tr>
        </thead>
        
    <% for (var e of events) { %>
        <tr>
            <td><%= e.Name_event %></td>
            <td><%= e.Name_place %></td>
            <td><%= e.Start_time %></td>
            <td><a href='/user/event/<%= e.Id_Event_schedule %>'>More informations</a></td>
        </tr>
    <% } %>
    </table>
</main>