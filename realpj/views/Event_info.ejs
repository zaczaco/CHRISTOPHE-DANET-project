<%- include('partial_header.ejs'); %>

<main>
    <table class="table table-striped table-bordered table-hover">
        <tr>
            <td>Name Event</td>
            <td>Superviser</td>
            <td>Start time</td>
            <td>End Time</td>
            <td>Price</td>
            <td>Name of the Place</td>
            <td>Capacity</td>
            <td>City</td>
        </tr>
        <tr>
            <td><%= e.Name_event %></td>
            <td><%= e.Superviser %></td>
            <td><%= e.Start_time %></td>
            <td><%= e.End_time %></td>
            <td><%= e.Price %></td>
            <td><%= e.Name_place %></td>
            <td><%= e.Capacity %></td>
            <td><%= e.City_place %></td>
        </tr>
    </table>

</main>
<main>


    

    <table class="table table-striped table-bordered table-hover">
        <tr>
            <td>Drink Name</td>
        </tr>
        <% for (var d of Drinks) { %>
        <tr>
            <td><%= d.Name_drink %></td>
        </tr>
        <% } %>
    </table>
</main>    
<main>
    

    <table class="table table-striped table-bordered table-hover">
        <tr>
            <td>Music Style</td>
            <td>Dj</td>
        </tr>
        <% for (var m of Musics) { %>
        <tr>
            <td><%= m.Style %></td>
            <td><%= m.Djs %></td>
        </tr>
        <% } %>
    </table>
</main>

